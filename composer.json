{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 10 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/acl": "^2.0",
        "drupal/admin_toolbar": "^3.6",
        "drupal/adsense": "^1.5",
        "drupal/advagg": "^6.0@alpha",
        "drupal/aggregator": "^2.2",
        "drupal/amazon_pa": "^2.1@beta",
        "drupal/audiofield": "^1.13",
        "drupal/auto_entitylabel": "^3.4",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bootstrap": "^5.0",
        "drupal/bootstrap_layout_builder": "^2.2",
        "drupal/cer": "^5.0@beta",
        "drupal/ckeditor5_plugin_pack": "^1.4",
        "drupal/ckeditor5_template": "^1.0",
        "drupal/coder": "^8.3",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "10.4.7",
        "drupal/core-project-message": "10.4.7",
        "drupal/core-recommended": "10.4.7",
        "drupal/dev_mode": "^2.6",
        "drupal/devel": "^5.0",
        "drupal/edit_plus": "^1.1",
        "drupal/entity_reference_revisions": "^1.11",
        "drupal/exclude_node_title": "^2.0",
        "drupal/feeds": "^3.0",
        "drupal/field_group": "^3.2",
        "drupal/field_permissions": "^1.4",
        "drupal/filecache": "^1.2",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/follow": "^2.0",
        "drupal/forum": "^1.0",
        "drupal/forum_access": "^2.0",
        "drupal/gin": "^4.1",
        "drupal/imagemagick": "^4.0",
        "drupal/insert_view": "^3.0",
        "drupal/job_scheduler": "^4.0",
        "drupal/lb_plus": "^3.3",
        "drupal/libraries": "^4.1",
        "drupal/mailsystem": "^4.5",
        "drupal/masquerade": "^2.0",
        "drupal/metatag": "^2.1",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.1",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/module_filter": "^5.0",
        "drupal/module_missing_message_fixer": "^2.0",
        "drupal/page_manager": "^4.0@RC",
        "drupal/panels": "^4.9",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.10",
        "drupal/php": "^1.2",
        "drupal/privatemsg": "^2.0@RC",
        "drupal/profile": "^1.12",
        "drupal/purge_users": "^4.0",
        "drupal/quicktabs": "^4.0",
        "drupal/quote": "^4.0",
        "drupal/radioactivity": "^4.1",
        "drupal/rdf": "^2.1",
        "drupal/recaptcha": "^3.4",
        "drupal/redirect": "^1.12",
        "drupal/rules": "^4.0",
        "drupal/scheduler": "^2.2",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "^1.38",
        "drupal/search_api_solr": "^4.3",
        "drupal/sharethis": "^3.1",
        "drupal/simpleads": "^2.0",
        "drupal/simplenews_scheduler": "^4.0@beta",
        "drupal/spamicide": "^1.0",
        "drupal/statistics": "^1.0",
        "drupal/statistics_counter": "^1.0",
        "drupal/subpathauto": "^1.4",
        "drupal/tagclouds": "^2.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/vbo_workflow_transition": "^1.0",
        "drupal/video_embed_field": "^2.5",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/webform": "^6.2",
        "drupal/workflow_buttons": "^1.0@beta",
        "drupal/xmlsitemap": "^1.2",
        "drupal/yoast_seo": "^2.1",
        "drush/drush": "^12"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "change search button": "patches/core/change_search_button.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
